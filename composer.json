{
    "name": "jjjjcccjjf/ldps-show-users",
    "description": "A wordpress plugin that utilises a static URL to display a virtual page of https://jsonplaceholder.typicode.com/users in table format",
    "authors": [
        {
            "name": "Lorenzo Salamante",
            "email": "lorenzodante.dev@gmail.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "jjjjcccjjf\\ShowUsers\\": ""
        }
    },
    "type": "wordpress-plugin",
    "license": "GPL-2.0+",
    "require": {
    	"php": ">=5.6",
        "composer/installers": "~1.0"
    },
    "require-dev": {
        "inpsyde/php-coding-standards": "^0.13.4",
        "brain/monkey": "2.*",
        "phpunit/phpunit": "5.7.9",
        "sebastian/resource-operations": "^1.0"
    },
    "post-install-cmd": [
        "composer dump-autoload -o"
    ]
}
